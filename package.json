{
  "name": "npm-playground",
  "version": "1.0.0",
  "description": "playing around with npm packages",
  "main": "./src/index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "tsc-watch --onSuccess \"npm start\"",
    "build": "tsc",
    "knex": "npx ts-node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile src/utils/databases/knex/knexfile.ts",
    "knex:migrate:make": "npm run knex -- migrate:make --",
    "knex:migrate:latest": "npm run knex -- migrate:latest",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "pretty": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "SnowSenpai",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-session-knex": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "objection": "^3.1.4",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
